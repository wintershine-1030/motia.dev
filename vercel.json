{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/docs",
      "dest": "/docs.html"
    },
    {
      "src": "/manifesto",
      "dest": "/manifesto.html"
    },
    {
      "src": "/rive/(.*)",
      "dest": "/rive/$1"
    },
    {
      "src": "/_next/(.*)",
      "dest": "/_next/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
