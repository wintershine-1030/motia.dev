{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "out"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/docs",
      "destination": "/docs.html"
    },
    {
      "source": "/manifesto",
      "destination": "/manifesto.html"
    },
    {
      "source": "/rive/(.*)",
      "destination": "/rive/$1"
    },
    {
      "source": "/_next/(.*)",
      "destination": "/_next/$1"
    },
    {
      "source": "/_vercel/(.*)",
      "destination": "/_vercel/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/rive/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
